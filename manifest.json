{
	"name": "SERP Ads",
	"description": "Extract Domains of Ads found on SERP",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": ["storage", "contextMenus", "activeTab", "scripting"],
	"host_permissions": [
		"*://*/*"
	],
	"action": {
		"default_popup": "popup.html",
		"default_title": "SERP Ads"
	}
}
  